	<queryString>
		<![CDATA[select
revenu.ID as ID,
TYPE,
PRIX as revenus,
0 as depenses,
DATES,
NOM_MUNI,ADRESSE,GOUVERNORAT,REGION,TEL,EMAIL,MAIRE_ACTUEL
from revenu,settings
union
select
depense.ID as ID,
TYPE,
0 as revenus,
PRIX*(-1) as depenses,
DATES,
NOM_MUNI,ADRESSE,GOUVERNORAT,REGION,TEL,EMAIL,MAIRE_ACTUEL
from depense,settings
order by DATES]]>
	</queryString>
